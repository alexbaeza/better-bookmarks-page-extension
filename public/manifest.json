{
  "name": "Better Bookmarks Page",
  "description": "A better start page for quick access.",
  "version": "1.3.0",
  "manifest_version": 3,
  "homepage_url": "https://github.com/alexbaeza/better-bookmarks-page-extension",
  "icons": {
    "16": "icons/logo16.png",
    "32": "icons/logo32.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html",
    "homepage": "index.html"
  },
  "permissions": [
    "bookmarks"
  ]
}
